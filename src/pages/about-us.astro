---
import BaseLayout from '../layouts/BaseLayout.astro';
---

<link rel="stylesheet" href="src/assets/about-us.css">
<BaseLayout>
    <div class="chain">
        <img class="chain-1" src="src/assets/FW_Chain.png" alt="">
        <img class="chain-2" src="src/assets/FW_Chain.png" alt="">
        <img class="chain-3" src="src/assets/FW_Chain.png" alt="">
    </div>
    <header>
        <h1 class="primary-text">About cs expo 2023</h1>
        <p class="secondary-text">The CS Expo showcases diverse students' innovative solutions in areas such as image processing and machine learning, with engaging presentations and insightful discussions. It's a must-attend event for an insightful experience.
            </p>

    </header>
    <main class="container">
        <section class="faculty-container">
            <h1 class="primary-text">
                faculty
            </h1>
            <div id="carouselExampleIndicators_faculty" class="carousel slide" data-bs-ride="true">
                <div class="carousel-indicators">
                  <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
                  <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="1" aria-label="Slide 2"></button>
                  <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="2" aria-label="Slide 3"></button>
                </div>
                <div class="carousel-inner">
                  <div class="carousel-item active">
                    <img src="src/assets/placeholder-image.png" class="d-block w-100" alt="...">
                  </div>
                  <div class="carousel-item">
                    <img src="src/assets/placeholder-image.png" class="d-block w-100" alt="...">
                  </div>
                  <div class="carousel-item">
                    <img src="src/assets/placeholder-image.png" class="d-block w-100" alt="...">
                  </div>
                </div>
                <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleIndicators_faculty" data-bs-slide="prev">
                  <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                  <span class="visually-hidden">Previous</span>
                </button>
                <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleIndicators_faculty" data-bs-slide="next">
                  <span class="carousel-control-next-icon" aria-hidden="true"></span>
                  <span class="visually-hidden">Next</span>
                </button>
              </div>
        </section>
        <section class="commitee-container">
            <h1 class="primary-text">
                commitee
            </h1>
            <div id="carouselExampleIndicators_commitee" class="carousel slide" data-bs-ride="true">
                <div class="carousel-indicators">
                  <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
                  <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="1" aria-label="Slide 2"></button>
                  <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="2" aria-label="Slide 3"></button>
                </div>
                <div class="carousel-inner">
                  <div class="carousel-item active">
                    <img src="src/assets/placeholder-image.png" class="d-block w-100" alt="...">
                  </div>
                  <div class="carousel-item">
                    <img src="src/assets/placeholder-image.png" class="d-block w-100" alt="...">
                  </div>
                  <div class="carousel-item">
                    <img src="src/assets/placeholder-image.png" class="d-block w-100" alt="...">
                  </div>
                </div>
                <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleIndicators_commitee" data-bs-slide="prev">
                  <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                  <span class="visually-hidden">Previous</span>
                </button>
                <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleIndicators_commitee" data-bs-slide="next">
                  <span class="carousel-control-next-icon" aria-hidden="true"></span>
                  <span class="visually-hidden">Next</span>
                </button>
              </div>
        </section>
    </main>
</BaseLayout>